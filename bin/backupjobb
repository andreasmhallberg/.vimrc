#!/bin/bash

TIME=`date +%y%m%d`

mkdir /Volumes/BACKUP1/jobb.$TIME

for F in \
.git \
.vim \
blog \
dotfiles \
jobb \
hobby-projects \
mylatexstuff \
mypandocstuff \
prosa
do
    echo Backar upp ${F}.
    tar -cpzf /Volumes/BACKUP1/jobb.$TIME/${F}.tar ~/$F
    echo Klar med ${F}.
done

echo Har backat upp allt jobbrelaterat.
